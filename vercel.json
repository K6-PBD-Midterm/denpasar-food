{
    "version": 2,
    "builds": [
      {
        "src": "denpasar_food/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb"
        }
      },
      {
        "src": "static/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/(.*)",
        "dest": "denpasar_food/wsgi.py"
      }
    ],
    "env": {
      "DJANGO_SETTINGS_MODULE": "denpasar_food.settings"
    }
  }